<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Suffix</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Body</th>
      <th>Mobile</th>
      <th>Email</th>
      <th>Website</th>
      <th>Facebook</th>
      <th>Instagram</th>
      <th>Line</th>
      <th>Whatsapp</th>
      <th>Roles</th>
      <th>Nick name</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.suffix %></td>
        <td><%= profile.first_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.age %></td>
        <td><%= profile.body %></td>
        <td><%= profile.mobile %></td>
        <td><%= profile.email %></td>
        <td><%= profile.website %></td>
        <td><%= profile.facebook %></td>
        <td><%= profile.instagram %></td>
        <td><%= profile.line %></td>
        <td><%= profile.whatsapp %></td>
        <td><%= profile.roles %></td>
        <td><%= profile.nick_name %></td>
        <td><%= profile.status %></td>
        <td><%= profile.user_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
